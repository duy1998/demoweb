            <div class="box">
              <div class="box-header">
                <h3 class="box-title">Danh Sách Sản Phẩm</h3>
                <a href="#" class="btn btn-sm btn-info btn-flat pull-right"
                data-toggle="modal" data-target="#dlginsertproduct"
                >Thêm Sản Phẩm Mới</a>
              </div>
              
              <div class="box-body">
                <table id="productTable" class="table table-bordered table-striped table-hover">
                  <thead>
                  <tr>
                    <th>ID</th>
                    <th>Tên Sản Phẩm</th>
                    <th>Loại Sản Phẩm</th>
                    <th>Nhà Sản Xuất</th>
                    <th>Hình Ảnh</th>
                    <th>Mô Tả</th>
                    <th>Giá Nhập</th>
                    <th>Giá Bán</th>
                    <th>Số Lượng</th>
                    <th>Trạng Thái</th>
                    <th>Cập Nhật</th>
                    <th>Xóa</th>
                  </tr>
                  </thead>
                  <tbody>
                      <% for(var i=0; i < listProduct.length; i++) {%>
                              <tr>
                                <td><%= listProduct[i].productID%></td>
                                <td><%= listProduct[i].productName%></td>
                                
                                <%if (listProduct[i].subCategoryName == null) { %>
                                  <td><%= listProduct[i].categoryName%></td>
                                  <% } else{ %>  %>
                                    <td><%= listProduct[i].subCategoryName%></td>
                                <% } %>
                                
                                <td><%= listProduct[i].manufacturer%></td>                            
                                <td><img width="64px" height="64px" src="<%= listProduct[i].image %>"/></td>
                                <td><span><%= listProduct[i].description%></span></td>
                            
                                <td><%= listProduct[i].importPrice%></td>
                                <td><%= listProduct[i].sellPrice%></td>
                                <td><%= listProduct[i].quantity%></td>
                                <td><%= listProduct[i].isActive%></td>
                                <td><Button class="btn btn-block btn-primary btn-sm"
                                  data-toggle="modal" data-target="#dlgdeleteproduct"
                                  >Chỉnh Sửa</Button></td>
                                <td><Button class="btn btn-block btn-danger btn-sm"
                                  data-toggle="modal" data-target="#dlgdeleteproduct"
                                  onclick="onShowDeleteProduct('<%= listProduct[i].productID%>','<%= listProduct[i].productName%>')"
                                  >Xóa</Button></td>
                              </tr>
                            <% } %>
